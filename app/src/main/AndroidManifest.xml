<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banchan.rssreaderL">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="RSS Reader LITE"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="RSS Reader LITE"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingFGActivity"
            android:label="設定"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RSS_resister"
            android:label="RSSの登録"
            android:screenOrientation="portrait" />
        <activity android:name=".RSS_Manager"
            android:label="RSSの表示/非表示設定"
            android:screenOrientation="portrait" />
    </application>

</manifest>
